{
	"name": "lockfile-tools",
	"version": "1.0.1",
	"description": "Utilities for parsing and working with npm ecosystem lockfiles",
	"exports": {
		"./package-managers": "./lib/package-managers.mjs",
		"./registry": "./registry.mjs",
		"./virtual": "./virtual.mjs",
		"./parsers": "./parsers.mjs",
		"./io": "./io.mjs",
		"./npm": "./npm.mjs",
		"./lib/*": "./lib/*",
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"version": "node ../../release.mjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/lockfile-tools.git",
		"directory": "packages/cli"
	},
	"keywords": [
		"lockfile",
		"npm",
		"yarn",
		"pnpm",
		"bun",
		"vlt",
		"parser"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/lockfile-tools/issues"
	},
	"homepage": "https://github.com/ljharb/lockfile-tools#readme",
	"dependencies": {
		"@hyrious/bun.lockb": "^0.0.4",
		"@npmcli/arborist": "^9.1.9"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.2",
		"@ljharb/tsconfig": "^0.3.2",
		"@types/npmcli__arborist": "^6.3.2",
		"auto-changelog": "^2.5.0",
		"eslint": "^9.39.2",
		"npmignore": "^0.3.5",
		"typescript": "next"
	},
	"engines": {
		"node": "^22.21.1 || ^24.12 || >= 25.2.1"
	},
	"publishConfig": {
		"ignore": []
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true,
		"appendGitLog": ".",
		"tagPrefix": "lockfile-tools@"
	}
}
